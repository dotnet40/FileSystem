{
  "version": "1.1.1-net40",
  "buildOptions": {
    "allowUnsafe": true,
    "warningsAsErrors": true,
    "keyFile": "../../tools/Key.snk",
    "xmlDoc": true
  },
  "description": "File provider for physical files for Microsoft.Extensions.FileProviders.",
  "packOptions": {
    "repository": {
      "type": "git",
      "url": "git://github.com/dotnet40/filesystem"
    },
    "tags": [
      "files",
      "filesystem"
    ]
  },
  "dependencies": {
    "Microsoft.Extensions.FileProviders.Abstractions": "1.1.1-net40",
    "Microsoft.Extensions.FileProviders.Sources": {
      "version": "1.1.1-net40",
      "type": "build"
    },
    "Microsoft.Extensions.FileSystemGlobbing": "1.1.1-net40"
  },
  "frameworks": {
    "net40": {
      "dependencies": {
        "System.Security.Cryptography.Algorithms": "4.3.0-net40"
      }
    }
  }
}